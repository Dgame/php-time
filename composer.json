{
    "name": "dgame/php-time",
    "description": "php time conversion",
    "license": "MIT",
    "type": "library",
    "homepage": "https://github.com/php-time",
    "keywords": [
        "Date",
        "Time",
        "Timer",
        "Clock",
        "Time conversion"
    ],
    "authors": [
        {
            "name": "Randy Sch√ºtt",
            "email": "rswhite4@gmail.com"
        }
    ],
    "require": {
        "php": "^7.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^6.4",
        "phpstan/phpstan": "^0.11.2"
    },
    "autoload": {
        "classmap": [
            "src/"
        ],
        "files": [
            "src/functions.php"
        ],
        "psr-4": {
            "Dgame\\Time\\": "src/"
        }
    },
    "scripts": {
        "test": [
            "phpunit",
            "phpstan analyse src --level=7"
        ],
        "coverage": [
            "phpunit --coverage-clover=coverage"
        ]
    }
}
